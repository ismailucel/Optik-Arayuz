@{
    var labels = (string[])TempData["labels"];
    var values = (string[])TempData["values"];
    bool flag = false;
    if (labels.Contains("ImagePath"))
    {
        flag = true;
    }

}
<head>
    <link rel="stylesheet" href="~/css/Components/Inputs.css" asp-append-version="true" />
</head>



    @for (var i = 0; i < Convert.ToInt32(ViewData["columnNumber"]); i++)
    {
        var id = "i" + i.ToString();
        <div class="inputs">
            <p>@labels[i]</p>
        @if (labels[i] == "ImagePath")
        {
            <input class="input" id="@id" type="text" disabled value="@values[i]">
        }
        else
        {
            <input class="input" id="@id" type="text" value="@values[i]">
        }
        </div>
    }
@if (flag)
{
        <form style="padding-left:40px;padding-bottom:10px" id="fileform" method="post" enctype="multipart/form-data" asp-controller="ExamPapers" asp-action="UploadLogo">
            <input style="margin-bottom:10px;" type="file" id="file" value="Yükle" />
            <input type="submit" value="Upload" />

        </form>

    

}
